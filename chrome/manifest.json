{
    "manifest_version": 2,

    "name": "Skiptco",
    "description": "This extension fixes twitter redirection links",
    "version": "1.2.2",

      "content_scripts": [
        {
          "matches": ["https://twitter.com/*","https://tweetdeck.twitter.com/*"],
          "js": ["skiptco_chrome.js"]
        }
      ],
    "permissions": [
        "tabs","https://twitter.com/*","https://tweetdeck.twitter.com/*"
        ]
}
